package sensehat;
struct imu
{
    double   timestamp;
    double   orientation[3];
    double   magnetic_compass[3]; 
    double   gyroscope[3];
    double   acceleration[3];
    string   orientation_string;
    string   magnetic_compass_string;
    string   gyroscope_string;
    string   acceleration_string;
}

struct env
{
    double   timestamp;
    double   humidity;
    double   temperature;
    double   pressure;
    string   humidity_string;
    string   temperature_string;
    string   pressure_string;
}